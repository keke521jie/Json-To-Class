{
  "name": "jsontoclass",
  "displayName": "Json To Class",
  "description": "And json to class",
  "version": "0.0.3",
  "publisher": "Yuanjie",
  "license": "MIT",
  "icon":"icon.png",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/keke521jie/Json-To-Class.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "explorer/context": [
        {
          "command": "jsontoclass.checkJsonFile",
          "group": "1_modification",
          "when": "resourceLangId == json",
          "context": [
            {
              "resourceLangId": "json"
            }
          ]
        }
      ]
    },
    "commands": [
      {
        "command": "jsontoclass.helloWorld",
        "title": "json to class"
      },
      {
        "command": "jsontoclass.checkJsonFile",
        "title": "Json To Class"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.84.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "typescript": "^5.3.2",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8"
  }
}
  